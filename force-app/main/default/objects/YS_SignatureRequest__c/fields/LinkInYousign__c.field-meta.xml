<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>LinkInYousign__c</fullName>
    <description>Link to view the Signature Request in Yousign</description>
    <externalId>false</externalId>
    <formula>IF(ISBLANK(ExternalId__c),
    null,
    HYPERLINK(&apos;https://yousign.app/auth/workspace/requests/&apos; &amp; ExternalId__c, &quot;Signature Request&quot;, &apos;_blank&apos;)
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Link in Yousign</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
